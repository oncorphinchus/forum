# Production environment variables
IS_PRODUCTION=true

# PostgreSQL database connection using Render's MySQL-style variables
MYSQL_HOST=YOUR_DATABASE_HOST # Replace with your database hostname during deployment
MYSQL_USER=YOUR_DATABASE_USER # Replace with your database username during deployment
MYSQL_PASSWORD=YOUR_DATABASE_PASSWORD # Replace with your database password during deployment
MYSQL_DATABASE=YOUR_DATABASE_NAME # Replace with your database name during deployment
DATABASE_PORT=5432 # PostgreSQL default port 